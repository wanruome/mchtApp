<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background"
    android:fitsSystemWindows="true"
    android:layout_height="match_parent">
    <com.ruomm.base.view.menutopview.MenuTopView
        android:id="@id/mymenutop"
        android:layout_width="match_parent"
        android:layout_height="@dimen/menutop_height"
        android:background="@color/AllTransparent"
        app:imageLeft="@drawable/menutop_left"
        app:menuButtomLineColor="@color/menutop_bottomline_color"
        app:menuImageSizePercent="0.7"
        app:menuPaddinghorizontal="@dimen/menutop_padding"
        app:menuTextColor="@color/menutop_textcenter_color"
        app:menuTextSize="@dimen/menutop_textsize"
        app:textLeft_Color="@color/menutop_textsider_color"
        app:textLeft_Size="@dimen/menutop_textsize_small"
        app:textRight_Color="@color/menutop_textsider_color"
        app:textRight_Size="@dimen/menutop_textsize_small"
        >
    </com.ruomm.base.view.menutopview.MenuTopView>
    <FrameLayout
        android:id="@+id/container_baseact"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </FrameLayout>
</LinearLayout>
