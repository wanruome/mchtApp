    </style>

    <style name="layoutPaddingStyle">
        <item name="android:paddingBottom">@dimen/padding_common_vertical</item>
        <item name="android:paddingTop">@dimen/padding_common_vertical</item>
        <item name="android:paddingLeft">@dimen/padding_common</item>
        <item name="android:paddingRight">@dimen/padding_common</item>
    </style>

