    <style name="editTextStyle">
        <item name="android:textColor">@color/text_white_sub</item>
        <item name="android:textColorHint">@color/text_lable</item>
        <item name="android:paddingLeft">@dimen/dpx010</item>
        <item name="android:paddingRight">@dimen/dpx010</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/textNormal</item>
        <item name="android:background">@drawable/bg_edittext</item>
    </style>

    <style name="btnStyle">
        <item name="android:layout_marginTop">@dimen/dpx020</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/textMiddle</item>
        <item name="android:textColor">@color/text_lable</item>
        <item name="android:background">@drawable/btn_bg_xml</item>
    </style>

    <style name="btnStyleNoMargin">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textMiddle</item>
        <item name="android:textColor">@color/text_lable</item>
        <item name="android:background">@drawable/btn_bg_xml</item>
    </style>

    <style name="btnSkipStyle">
        <item name="android:layout_height">@dimen/line_dpx_1</item>
        <item name="android:layout_width">@dimen/dpx032</item>
    </style>

    <style name="pwdBtnStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textSize">@dimen/textMiddle</item>
        <item name="android:background">@drawable/btn_rand_password</item>
    </style>
    <style name="pwdBtnVerify">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/textSmall</item>
        <item name="android:textColor">@color/text_white_sub</item>
        <item name="android:background">@drawable/btn_verifycode</item>
    </style>
    <!-- progressBar_ -->
    <style name="progressBar_downLoading" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/common_progressbar_download</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">10dip</item>
    </style>
